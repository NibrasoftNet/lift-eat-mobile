{
  "expo": {
    "name": "lift&eat",
    "slug": "L&E",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "splash": {
      "image": "./assets/images/auth/login_background.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#21273F"
    },
    "scheme": "lift-and-eat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpg"
      },
      "package": "com.nibrasoftnet.liftandeat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_no_bg.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-localization",
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "581cddfa-2371-4db1-a11c-0a98f258f6e5"
      }
    }
  }
}
