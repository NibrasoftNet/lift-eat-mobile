{
  "expo": {
    "name": "lift-eat",
    "slug": "fitness",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/auth/login_background.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#21273F"
    },
    "scheme": "Lift&Eat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-localization",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
